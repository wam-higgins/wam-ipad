<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">

<!--
	IMPORTANT INFO: 
	1. "clear-current-content" if set to true will DELETE EVERYTTHING.
	2. Kiosk Pro only updates ONE zip at a time, no matter how you manipulate the version numbers. 
 -->
	
    <array>
   	<dict> <!--Main HTML and static images -->
            <key>version</key>
            <integer>5</integer>
            <key>zip</key>
            <string>https://github.com/wam-higgins/wam-ipad/archive/refs/heads/main.zip</string>
            <key>clear-previous-content</key>
            <true/>
        </dict>

        <dict> <!--Home page video and screensaver-->
            <key>version</key>
            <integer>6</integer>
            <key>zip</key>
            <string>https://github.com/wam-higgins/wam-ipad-videos/archive/refs/heads/main.zip</string>
            <key>clear-previous-content</key>
            <false/>
        </dict>
	    
	<dict> <!-- Behind the Scenes Videos -->
            <key>version</key>
            <integer>5</integer>
            <key>zip</key>
            <string>https://github.com/wam-higgins/bts-video/archive/refs/heads/main.zip</string>
            <key>clear-previous-content</key>
            <false/>
        </dict>

	<dict> <!--Spotlight Videos -->
            <key>version</key>
            <integer>4</integer>
            <key>zip</key>
            <string>https://github.com/wam-higgins/spotlight-video/archive/refs/heads/main.zip</string>
            <key>clear-previous-content</key>
            <false/>
        </dict>

    </array>
</plist>
